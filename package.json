{
	"name": "schdesign-web",
	"version": "2.0.0",
	"private": true,
	"description": "Website of schdesign",
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true pnpm run build",
		"clean": "git clean --interactive -dx --exclude .env.local --exclude .env.*.local --exclude node_modules/ --exclude .husky/",
		"dev": "next dev",
		"fix": "run-s fix:*",
		"fix:eslint": "pnpm run lint:eslint --fix",
		"fix:prettier": "pnpm run lint:prettier --write",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore .",
		"lint:prettier": "prettier --check .",
		"lint:typescript": "tsc",
		"prepare": "husky install",
		"start": "next start"
	},
	"nano-staged": {
		"*": "prettier --write --ignore-unknown",
		"*.{ts,tsx,js,jsx}": "eslint --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/simonyiszk/schdesign-web.git"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/simonyiszk/schdesign-web/issues"
	},
	"homepage": "https://schdesign.hu",
	"browserslist": [
		"defaults",
		"maintained node versions",
		"last 2 versions",
		"not IE <=11"
	],
	"dependencies": {
		"@mdx-js/loader": "^2.1.3",
		"@mdx-js/react": "^2.1.3",
		"@mui/lab": "^5.0.0-alpha.97",
		"@mui/material": "^5.10.3",
		"@next/bundle-analyzer": "12.2.5",
		"@next/mdx": "^12.2.5",
		"@tailwindcss/aspect-ratio": "^0.4.1",
		"autoprefixer": "^10.4.8",
		"clsx": "1.2.1",
		"focus-visible": "5.2.0",
		"next": "12.2.5",
		"next-compose-plugins": "^2.2.1",
		"next-mdx-remote": "^4.1.0",
		"postcss": "^8.4.16",
		"postcss-import": "15.0.0",
		"postcss-preset-env": "7.8.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.4.0",
		"sass": "^1.54.8",
		"tailwindcss": "3.1.8"
	},
	"devDependencies": {
		"@tsconfig/next": "1.0.2",
		"@types/node": "16.11.56",
		"@types/react": "18.0.18",
		"@typescript-eslint/eslint-plugin": "5.36.1",
		"@typescript-eslint/parser": "5.36.1",
		"cross-env": "7.0.3",
		"eslint": "8.23.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-next": "12.2.5",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.1",
		"eslint-plugin-react": "7.31.1",
		"eslint-plugin-react-etc": "2.0.0",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-simple-import-sort": "7.0.0",
		"husky": "8.0.1",
		"nano-staged": "0.8.0",
		"npm-run-all": "4.1.5",
		"prettier": "2.7.1",
		"prettier-plugin-tailwindcss": "0.1.13",
		"typescript": "4.8.2"
	}
}
